	"scripts": {
		"ts-node": "ts-node -r tsconfig-paths/register",
		"prepare": "husky install",
		"build": "serverless package",
		"start": "node dist/index.js",
		"create:layer": "mkdir -p layers/modules && cp package.json layers/modules/package.json && cp yarn.lock layers/modules/yarn.lock && cd layers/modules && yarn install --production && cd ../..",
		"deploy:prd:auth": "DEPLOY_TYPE=AUTH NODE_ENV=production serverless deploy --stage production",
		"deploy:dev:auth": "DEPLOY_TYPE=AUTH NODE_ENV=dev serverless deploy --stage dev",
		"deploy:local:auth": "DEPLOY_TYPE=AUTH NODE_ENV=local serverless deploy --stage local",
		"docker:server": "docker-compose up --build docker-events-listener localstack",
		"docker:migrations": "docker-compose up --build run-migrations",
		"docker:func": "docker-compose up --build api",
		"format": "eslint . --fix --quiet",
		"lint": "eslint . --quiet",
		"test": "jest --passWithNoTests",
		"test:cov": "jest --coverage --passWithNoTests",
		"upgrade-dependencies": "yarn upgrade-interactive --latest",
		"create:migration": "node scripts/gen-migration.js",
		"create:seed": "node scripts/gen-seed.js",
		"internal:docker:run:migrations": "ts-node src/database/migrations/index.ts",
		"internal:local": "yarn deploy:local",
		"temp:test-tsc": "tsc",
		"husky:pre-commit": "tsc && yarn lint",
		"cln:system": "docker system prune -f",
		"cln:volume": "docker volume prune -f",
		"cln:cache": "sudo rm -rf .serverless/ layers/ tmp/",
		"cln:all": "yarn cln:system && yarn cln:volume && yarn cln:cache"
	}
}
